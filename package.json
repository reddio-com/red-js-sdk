{
  "version": "0.1.0",
  "name": "@reddio/js-sdk",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "xuwu",
  "module": "dist/mylib.esm.js",
  "size-limit": [
    {
      "path": "dist/mylib.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/mylib.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@depay/web3-blockchains": "^5.0.1",
    "@depay/web3-mock": "^11.8.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/bn.js": "^5.1.0",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@starkware-industries/starkware-crypto-utils": "^0.0.2",
    "axios": "^0.27.2",
    "bn.js": "^5.2.1",
    "ethers": "^5.6.8"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setup.ts"
    ]
  }
}
