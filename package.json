{
  "version": "0.2.0",
  "name": "@reddio.com/js",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "private": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "deploy-example": "cd example && yarn && yarn build"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "xuwu",
  "module": "dist/js.esm.js",
  "size-limit": [
    {
      "path": "dist/js.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/js.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/plugin-transform-template-literals": "^7.18.2",
    "@changesets/cli": "^2.23.2",
    "@depay/web3-blockchains": "^5.0.1",
    "@depay/web3-mock": "^11.8.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/bn.js": "^5.1.0",
    "husky": "^8.0.1",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@starkware-industries/starkware-crypto-utils": "^0.0.2",
    "assert": "^2.0.0",
    "axios": "^0.27.2",
    "bn.js": "^5.2.1",
    "enc-utils": "^3.0.0",
    "ethers": "^5.6.8"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setup.ts"
    ]
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.11.1",
    "**/@typescript-eslint/parser": "^4.11.1",
    "**/jest": "^26.6.3",
    "**/ts-jest": "^26.4.4",
    "**/typescript": "^4.1.3"
  }
}
